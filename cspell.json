{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "words": [
    "commitlint",
    "Elsayed",
    "parens",
    "postversion",
    "prebuild",
    "preversion",
    "tailwindcss",
    "eslint",
    "config",
    "sonarjs"
  ],
  "language": "en",
  "ignoreRegExpList": ["/[ุก-ู]+/"],
  "files": ["**"],
  "enableGlobDot": true,
  "ignorePaths": [
    "node_modules",
    "dist",
    ".idea",
    ".git",
    ".angular",
    ".gitignore",
    ".husky/_/**",
    ".eslintrc.*",
    "**/*.svg",
    "decorate-angular-cli.js",
    ".browserslistrc",
    "cspell.json",
    ".vscode/**",
    "jest.config.ts",
    "android",
    "ios",
    ".scannerwork",
    "nginx*.conf"
  ]
}
